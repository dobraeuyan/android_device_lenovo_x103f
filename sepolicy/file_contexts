# Dev nodes
/dev/media([0-2])+				u:object_r:video_device:s0

# persist files
/persist/misc(/.*)?				u:object_r:persist_misc_file:s0
/persist/wlan_mac.bin				u:object_r:wifi_data_file:s0

# firmware images
/firmware/image(/.*)?				u:object_r:firmware_file:s0
/system/etc/firmware(/.*)?			u:object_r:firmware_file:s0

# proprietary bin
/system/bin/serialno				u:object_r:serialno_exec:s0

# WiFi
/system/bin/macplugin				u:object_r:macplugin_exec:s0
