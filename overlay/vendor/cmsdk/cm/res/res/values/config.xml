    <!-- Color temperature settings for LiveDisplay. These were obtained by
         measuring the display at various color balance levels. -->
    <integer name="config_minColorTemperature">3000</integer>
    <integer name="config_maxColorTemperature">9500</integer>

    <!-- Corresponds to color balance level of zero, this is the native
         display temperature -->
    <integer name="config_dayColorTemperature">5400</integer>
    <integer name="config_nightColorTemperature">4500</integer>

</resources>
